{
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "categories": [
        {
            "title": "## 🚀 New Features",
            "labels": ["feature"]
        },
        {
            "title": "## 🐛 Fixed Bugs",
            "labels": ["bug"]
        }
    ],
    "template": "${{CATEGORIZED_COUNT}} changes since ${{FROM_TAG}}\n\n${{CHANGELOG}}",
    "transformers": [
      {
        "pattern": "(.) (.+)", 
        "target": "$1"
      }
    ]
}
