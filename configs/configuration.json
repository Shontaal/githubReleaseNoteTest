{
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "categories": [
        {
            "title": "## 🚀 New Features",
            "labels": ["feature"]
        },
        {
            "title": "## 🐛 Fixed Bugs",
            "labels": ["bug"]
        },
        {
            "title": "## 💬 Other",
            "labels": []
        }
    ],
    "template": "${{CATEGORIZED_COUNT}} changes since ${{FROM_TAG}}\n\n${{CHANGELOG}}",
    "transformers": [
      {
        "pattern": "AB#([0-9]+)( )?(.+?)",
        "target": "<a target='_blank' href='https://dev.azure.com/shontaal/releaseNotesTest/_workitems/edit/$1'>AB#$1</a> - $3"
      }
    ]
}
